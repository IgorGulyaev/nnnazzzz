name = ZEN Landing Page Theme
description = A Bootstrap Sub-theme.
core = 7.x
engine = phptemplate
base theme = bootstrap

;;;;;;;;;;;;;;;;;;;;;
;; Regions
;;;;;;;;;;;;;;;;;;;;;

regions[header]         = 'Header'
regions[highlighted]    = 'Highlighted'
regions[help]           = 'Help'
regions[prefix]         = 'Portfolio'
regions[content]        = 'Content'
regions[suffix]         = 'Suffix'
regions[featured]       = 'Featured'
regions[footer]         = 'Footer'

;;;;;;;;;;;;;;;;;;;;;
;; Dependencies
;;;;;;;;;;;;;;;;;;;;;
dependencies[] = libraries (>=2)
dependencies[] = jquery_update (>=2)

;;;;;;;;;;;;;;;;;;;;;
;; Stylesheets
;;;;;;;;;;;;;;;;;;;;;
stylesheets[all][] = css/masterslider.css
stylesheets[all][] = css/jquery.bxslider.min.css
stylesheets[all][] = css/colors.css
stylesheets[all][] = css/animate.css
stylesheets[all][] = css/style.css
;stylesheets[all][] = css/custom_style/custom_style.css

;;;;;;;;;;;;;;;;;;;;;
;; Javascripts
;;;;;;;;;;;;;;;;;;;;;
scripts[] = js/anchors_region_navigation.js
scripts[] = js/jquery.appear.js
scripts[] = js/jquery.cookie.js
scripts[] = js/masterslider/modernizr-custom.js
scripts[] = js/masterslider/jquery.easing.min.js
scripts[] = js/masterslider/masterslider.min.js
scripts[] = js/masterslider/masterslider.staff.carousel.js
scripts[] = js/html5shiv.js
scripts[] = js/modernizr.custom.js
scripts[] = js/jquery.bxslider.min.js
scripts[] = js/device.min.js
scripts[] = js/jquery.touchSwipe.js
scripts[] = js/jquery.event.frame.js
scripts[] = js/jquery.parallax.min.js
scripts[] = js/ScrollToPlugin.min.js
scripts[] = js/TweenMax.min.js
scripts[] = js/wow.js
scripts[] = js/script.js

; Landing page theme settings
; --------------
settings[default_background] = 1

; Region paddings
; --------------
settings[region_padding_top] = 100
settings[region_padding_bottom] = 100

; Region anchor
; --------------
settings[region_anchor-header] = 'header'
settings[region_anchor-highlighted] = 'highlighted'
settings[region_anchor-help] = 'help'
settings[region_anchor-prefix] = 'prefix'
settings[region_anchor-content] = 'content'
settings[region_anchor-suffix] = 'suffix'
settings[region_anchor-featured] = 'featured'
settings[region_anchor-footer] = 'footer'
; Information added by Drupal.org packaging script on 2015-07-24
version = "7.x-1.x-dev"
core = "7.x"
project = "landing_page_theme"
datestamp = "1437734643"

